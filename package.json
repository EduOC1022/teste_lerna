{
  "name": "teste_lerna",
  "packageManager": "yarn@4.2.2",
  "workspaces": [
    "packages/*"
  ],
  "devDependencies": {
    "concurrently": "8.2.2",
    "nx": "19.0.4"
  },
  "scripts": {
    "start-backend": "cd packages && cd backend && node app",
    "start-frontend": "cd packages && cd frontend && yarn start",
    "dev": "concurrently \"yarn run start-backend\" \"cd ..\" \"yarn run start-frontend\""
  },
  "dependencies": {
    "backend": "*",
    "hapi-cors-headers": "^1.0.3",
    "lerna": "^8.1.3",
    "nx": "^19.0.4"
  }
}
